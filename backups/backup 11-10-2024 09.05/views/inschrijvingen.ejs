<!DOCTYPE html>
<html lang="nl">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %></title>
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <header>
        <h1><%= title %></h1>
        <a href="/admin-dashboard">Terug naar dashboard</a>
    </header>
    <table>
        <thead>
            <tr>
                <th>Gebruiker</th>
                <th>Activiteit</th>
                <th>Datum</th>
            </tr>
        </thead>
        <tbody>
            <% inschrijvingen.forEach(inschrijving => { %>
                <tr>
                    <td><%= inschrijving.email %></td>
                    <td><%= inschrijving.activiteit %></td>
                    <td><%= inschrijving.datum %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>
